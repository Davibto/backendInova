generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Modelo do banco de dados com id gerado no formato UUID, nome, email unico e senha
model User {
  id String @id @default(uuid()) @db.Uuid // identificador Ãºnico universal
  name String
  email String @unique
  password String
}
